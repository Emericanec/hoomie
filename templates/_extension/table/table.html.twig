<div class="card">
    <!-- /.card-header -->
    <div class="card-body">
        <div class="dataTables_wrapper dt-bootstrap4">
            <div class="row">
                <div class="col-sm-12">
                    <table class="table table-bordered table-hover dataTable" role="grid">
                        <thead>
                        <tr role="row">
                            {% for column in options.columns %}
                                <th class="sorting_asc" tabindex="0" rowspan="1" colspan="1">{{ column.title }}</th>
                            {% endfor %}
                        </tr>
                        </thead>
                        <tbody>
                        {% for one in data %}
                            <tr role="row" class="odd">
                                {% for column in options.columns %}
                                    <td>{{ attribute(one, column.getter) }}</td>
                                {% endfor %}
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
